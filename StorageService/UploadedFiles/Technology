akamai.amp.AMP.loadDefaults("/assets/player/amp/2.19.0.0004/_config/default.xml?v=4", function () {
amp = akamai.amp.AMP.create("mediaplayer", config_overrides, readyHandler);
if(typeof swfobject != "undefined"){
swfobject.callDomLoadFunctions();
}
});
function readyHandler(event){
//bind page level variables to the player
amp.setParams({siteSectionId:abcnws_fw_params.siteSectionId});
amp.setParams({referer : "http://abcnews.go.com/US/video/spacex-rocket-explodes-international-space-station-32083892"});
amp.setParams({playerLocation : "player|storypage"});
amp.setParams({videoViewEvent:"event1"});
amp.addEventListener("canplay", on_can_play);
amp.addEventListener("playrequest", on_playRequest);
amp.addEventListener("play", on_play);
amp.addEventListener("pause", on_paused);
amp.addEventListener("ended", on_end);
amp.addEventListener("volumechange", on_volumeChanged);
if(amp.ads!=null){
amp.ads.addEventListener("started", on_adStart);
amp.ads.addEventListener("ended", on_adEnd);
amp.ads.addEventListener("companion", companionHandler);
}
if (amp.share != null){
amp.share.addEventListener("share", shareHandler);
}
if (amp.autoadvance != null){
amp.autoadvance.addEventListener("advance", advanceHandler);
}
if (amp.recommendations != null){
amp.recommendations.addEventListener("selected", recSelectedHandler);
}
storyPlayer.ready();
}
